# Debug build directory
DEBUG_DIR=debug

# Release build directory
RELEASE_DIR=release

# Compilers
CXX_COMPILER=g++-4.8
C_COMPILER=gcc-4.8

# Flags passed to C++ compiler
CXXFLAGS=

# Root path to find cuda, if you installed it into some other places
CUDA_ROOT=

# Root path to find cudnn
CUDNN_ROOT=

# Python version for owl package.
# ATTENTION: this version should be consistent with the version used when building
#            boost_python library
# If not set, the version used in building will depend on FindPythonLibs.cmake
PYTHON_VERSION=

# Semicolon seperated list of external dependencies, e.g. cuDNN
# Double quote paths to make it a single string
EXTERN_INCLUDE_PATH="/home/cs_user/Libs/cudnn-6.5-linux-R1"
EXTERN_LIB_PATH="/home/cs_user/Libs/cudnn-6.5-linux-R1"
